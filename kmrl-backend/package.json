{
  "name": "kmrl-backend",
  "version": "1.0.0",
  "description": "Production-ready backend system for KMRL Document Management System",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npm run build && node dist/config/migrate.js",
    "seed": "npm run build && node dist/config/seed.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": ["document-management", "ai", "classification", "nodejs", "typescript"],
  "author": "KMRL Development Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "mongodb": "^6.3.0",
    "redis": "^4.6.11",
    "elasticsearch": "^16.7.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^5.0.4",
    "socket.io": "^4.7.4",
    "bull": "^4.12.2",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "@types/node": "^20.10.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.10.9",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}